{
  "private": true,
  "name": "movemania",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:clean": "expo start --clear",
    "prebuild": "npx expo prebuild",
    "prebuild:clean": "npx expo prebuild --clean",
    "prebuild:ci": "EXPO_NO_GIT_STATUS=1 npx expo prebuild --clean --skip-dependency-update react,react-native",
    "android": "npx expo run:android",
    "android:clean": "npx expo run:android --no-build-cache",
    "ios": "npx expo run:ios",
    "ios:clean": "npx expo run:ios --no-build-cache",
    "lint": "eslint './src/**/*.{ts,tsx}' --ignore-path .eslintignore",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"src/**/*.{ts,tsx}\"",
    "traveler": "node ./scripts/traveler.mjs"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.19.5",
    "expo": "49.0.19",
    "expo-av": "~13.4.1",
    "expo-blur": "~12.4.1",
    "expo-font": "~11.4.0",
    "expo-image": "~1.5.1",
    "expo-location": "~16.1.0",
    "expo-status-bar": "~1.6.0",
    "expo-task-manager": "~11.3.0",
    "h3-js": "4.1.0",
    "lottie-react-native": "6.4.1",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-maps": "1.8.0",
    "react-native-reanimated": "3.3.0",
    "react-native-svg": "13.9.0",
    "stitches-native": "0.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@taito/eslint-config-react-native": "github:TaitoUnited/taito-eslint-config-react-native",
    "@types/react": "18.2.14",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "arg": "5.0.2",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-lodash": "7.4.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-standard": "5.0.0",
    "prettier": "3.1.0",
    "typescript": "5.2.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "engines": {
    "npm": ">=9",
    "node": ">=18"
  }
}
